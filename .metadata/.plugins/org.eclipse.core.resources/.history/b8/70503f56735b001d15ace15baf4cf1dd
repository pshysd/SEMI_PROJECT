package com.kh.statistics.model.vo;

public class Sales {

    private int today;
    private int weeks;
    
    public Sales() {
        
    }

    public Sales(int today, int weeks) {
        this.today = today;
        this.weeks = weeks;
    }

    public int getToday() {
        return today;
    }

    public void setToday(int today) {
        this.today = today;
    }

    public int getWeeks() {
        return weeks;
    }

    public void setWeeks(int weeks) {
        this.weeks = weeks;
    }

    @Override
    public String toString() {
        return "Sales [today=" + today + ", weeks=" + weeks + "]";
    }
    
    
}
